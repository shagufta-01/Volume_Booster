{
    "name": "Volume Booster",
    "manifest_version": 3,
    "description": "this is my volume booster",
    "version": "1.0.0",
    "action": {
        "default_title": "Volume Booster Extension",
        "default_icon": {
            "19": "icons/icon-logo.png",
            "38": "icons/icon-logo.png"
        }
    },
    "icons": {
		"16": "icons/icon-logo.png",
		"32": "icons/icon-logo.png",
		"48": "icons/icon-logo.png",
		"128": "icons/icon-logo.png"
	},
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "tabCapture"
    ],
    "background": {
        "service_worker": "background.js"
    }
}